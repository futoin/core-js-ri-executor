    "grunt-mocha-phantomjs": "*",
    "grunt-npm-helper": "*",
    "grunt-npm2bower-sync": "^0.8.1",
    "grunt-pure-cjs": "*",
    "grunt-release": "*",
    "grunt-text-replace": "*",
    "istanbul": "^0.3.6",
    "jscs": "*",
    "jsdoc": "^3.2.2",
    "jsdoc-to-markdown": "^0.5.11",
    "jshint": "*",
    "jshint-full-path": "*",
