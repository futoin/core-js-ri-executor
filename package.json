    "futoin-asyncsteps": "^1.5.4",
    "futoin-invoker": ">=0.7.0",
    "lodash": "^3.3.0",
    "performance-now": "*"
  },
  "devDependencies": {
    "chai": "^2.1.0",
    "grunt": "*",
